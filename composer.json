{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "ext-PDO": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-json": "*",
        "ext-rdkafka": "*",
        "ext-pcntl": "*",
        "eventmosh/elog-sdk-php": "^1.0",
        "fideloper/proxy": "~3.3",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/mongodb": "3.3.*",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "league/fractal": "^0.17.0",
        "predis/predis": "^1.1",
        "spatie/laravel-activitylog": "^3.2",
        "tucker-eric/eloquentfilter": "^1.4"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "~1.4",
        "kwn/php-rdkafka-stubs": "^1.2",
        "mockery/mockery": "~1.0",
        "overtrue/phplint": "^1.1",
        "phpunit/phpunit": "~6.0",
        "shixinke/php-ide-helper": "dev-master",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.development', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "codestyle": [
            "/usr/bin/php ./vendor/bin/phplint",
            "/usr/bin/php ./vendor/bin/php-cs-fixer fix --config=.php_cs.dist"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:eventmosh/elog-sdk-php.git"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
